{
	"openapi": "3.0.0",
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"UserInfoResponse": {
				"properties": {
					"username": {
						"type": "string"
					},
					"profilePicture": {
						"type": "string"
					}
				},
				"required": [
					"username",
					"profilePicture"
				],
				"type": "object",
				"additionalProperties": false
			},
			"AuthenticationResponse": {
				"properties": {
					"ssoToken": {
						"type": "string",
						"description": "If an SSO token is requested, this will be populated with an SSO token\r\nthat can be used to authenticate once."
					}
				},
				"type": "object",
				"additionalProperties": false
			},
			"ApiError": {
				"properties": {
					"name": {
						"type": "string"
					},
					"message": {
						"type": "string"
					},
					"stack": {
						"type": "string"
					}
				},
				"required": [
					"name",
					"message"
				],
				"type": "object",
				"additionalProperties": false
			},
			"RegisterRequest": {
				"properties": {
					"username": {
						"type": "string",
						"description": "The username other users will see this user represented by"
					},
					"password": {
						"type": "string",
						"description": "The password the user will be using to authenticate"
					},
					"email": {
						"type": "string",
						"description": "The email the user wishes to receive notifications on"
					},
					"birthday": {
						"type": "number",
						"format": "double",
						"description": "The time in unix epoch that is this user's birthday."
					},
					"ssoRequest": {
						"type": "boolean",
						"description": "If set to true, an SSO will be returned on login"
					}
				},
				"required": [
					"username",
					"password",
					"email",
					"birthday"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {
			"api_key": {
				"type": "apiKey",
				"name": "access_token",
				"in": "query"
			}
		}
	},
	"info": {
		"title": "@mud-portal/backend",
		"version": "1.0.0",
		"license": {
			"name": "ISC"
		},
		"contact": {}
	},
	"paths": {
		"/worlds/connect": {
			"post": {
				"operationId": "Connect",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"type": "string"
								}
							}
						}
					}
				},
				"security": [
					{
						"jwt": []
					}
				],
				"parameters": []
			}
		},
		"/users/info/{userId}": {
			"get": {
				"operationId": "GetUserInfo",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/UserInfoResponse"
								}
							}
						}
					}
				},
				"security": [],
				"parameters": []
			}
		},
		"/users/login": {
			"post": {
				"operationId": "LoginUser",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/AuthenticationResponse"
								}
							}
						}
					},
					"401": {
						"description": "InvalidPassword",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ApiError"
								}
							}
						}
					}
				},
				"security": [],
				"parameters": []
			}
		},
		"/users/register": {
			"post": {
				"operationId": "Register",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/AuthenticationResponse"
								}
							}
						}
					},
					"400": {
						"description": "PasswordInvalid",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ApiError"
								}
							}
						}
					}
				},
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/RegisterRequest"
							}
						}
					}
				}
			}
		}
	},
	"servers": [
		{
			"url": "/"
		}
	]
}